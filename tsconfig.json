{
  "compilerOptions": {
    "target": "es2021",                                 
    "module": "esnext",                               
	"moduleResolution": "nodenext",
	"declaration": true,                             
    "sourceMap": true,                               
    "declarationDir": "./types",                          
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                     
    "skipLibCheck": true                                
  }
}
